  xmlns:rdfs="&rdfs;#" 
  xmlns:owl="&owl;#" 
  xmlns:xsd="&xsd;#" 
  xmlns:service="&service;#" 
  xmlns:grounding="&grounding;#" 
  xmlns:ba_service="&ba_service;#" 
