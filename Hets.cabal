name: Hets
version: 0.87
Cabal-Version: >= 1.2.2
License-File: LICENSE.txt
Description: The Heterogeneous Tool Set
build-type: Simple
build-depends:
    base >= 3.0.1.0
  , array >= 0.1.0.0
  , containers >= 0.1.0.1
  , directory >= 1.0.0.0
  , pretty >= 1.0.0.0
  , process >= 1.0.0.0
  , haskell98 >= 1.0.1.0
  , mtl >= 1.1.0.0
  , parsec >= 2.1.0.0
  , regex-compat >= 0.71.0.1
  , fgl >= 5.4.1.1
  , time >= 1.1.2.0
  , old-time >= 1.0.0.0
  , unix >= 2.3.0.0
  , network >= 2.1.0.0
  , dataenc >= 0.11
  , Shellac >= 0.9
  , Shellac-compatline >= 0.9
  , HaXml >= 1.13.2
  , HaXml <= 1.13.3
  , HTTP >= 3001.0.4
  , hxt >= 8.0.0
  , hxt-filter >= 8.0.0
  , uni-htk >= 1.1
  , uni-util
  , uni-posixutil
  , uni-events
  , uni-reactor
  , uni-graphs >= 1.1
  , uni-davinci >= 1.1
  , HAIFA >= 0.11
  , syb-generics == 2.9
  , programatica == 1.0
executable: hets
main-is: hets.hs
extensions: OverlappingInstances, CPP
ghc-options: -fglasgow-exts
cpp-options: -DUNI_PACKAGE
  -DCASLEXTENSIONS -DPROGRAMATICA -DHAXML_PACKAGE
